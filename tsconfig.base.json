{
	"compilerOptions": {
		// "allowJs": true,
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
    	"forceConsistentCasingInFileNames": true,
		"module": "ES2022",
		"moduleResolution": "Node",
		"pretty": true,
		"preserveConstEnums": true,
		"sourceMap": true,
		"removeComments": true,
		"strict": true,
		"target": "es2018",
		"useDefineForClassFields": true,
	},
}
